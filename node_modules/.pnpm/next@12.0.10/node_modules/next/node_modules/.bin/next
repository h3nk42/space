#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/dist/bin/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/dist/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules:/Users/neekh/htw/other/portfolio/node_modules:/Users/neekh/htw/other/node_modules:/Users/neekh/htw/node_modules:/Users/neekh/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/dist/bin/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/dist/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules/next/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/next@12.0.10/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules/.pnpm/node_modules:/Users/neekh/htw/other/portfolio/space/node_modules:/Users/neekh/htw/other/portfolio/node_modules:/Users/neekh/htw/other/node_modules:/Users/neekh/htw/node_modules:/Users/neekh/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/bin/next" "$@"
else
  exec node  "$basedir/../../dist/bin/next" "$@"
fi
